<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ixiamen.activity.mapper.IntegralMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ixiamen.activity.entity.Integral">
        <id column="integral_id" property="integralId" />
        <result column="user_id" property="userId" />
        <result column="user_name" property="userName" />
        <result column="phone" property="phone" />
        <result column="mobile" property="mobile" />
        <result column="integral_count" property="integralCount" />
        <result column="integral_source" property="integralSource" />
        <result column="integral_time" property="integralTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        integral_id AS integralId, user_id AS userId, user_name AS userName, phone, mobile, integral_count AS integralCount, integral_source AS integralSource, integral_time AS integralTime
    </sql>

</mapper>
